@model QuizApplication.Models.DashboardViewModel

<main id="main" class="main"> 

     <div class="pagetitle"> 
         <h1>Dashboard</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
                <li class="breadcrumb-item active">Dashboard</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->

    @* <div class="pagetitle"> *@
    @*     <h1>Dashboard</h1> *@
    @*     <nav> *@
    @*         <ol class="breadcrumb"> *@
    @*             <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Admin</a></li> *@
    @*             <li class="breadcrumb-item active">Dashboard</li> *@
    @*         </ol> *@
    @*     </nav> *@
    @* </div><!-- End Page Title --> *@

    <section class="section dashboard">
        <div class="row">
            <!-- Total Quizzes -->
            <div class="col-xxl-4 col-md-4">
                <div class="card info-card sales-card">
                    <div class="card-body">
                        <h5 class="card-title">Total Quizzes</h5>
                        <div class="d-flex align-items-center">
                            <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                                <i class="bi bi-book"></i>
                            </div>
                            <div class="ps-3">
                                <h5>@Model.TotalQuizzes</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Total Questions -->
            <div class="col-xxl-4 col-md-4">
                <div class="card info-card revenue-card">
                    <div class="card-body">
                        <h5 class="card-title">Total Questions</h5>
                        <div class="d-flex align-items-center">
                            <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                                <i class="bi bi-question-circle"></i>
                            </div>
                            <div class="ps-3">
                                <h5>@Model.TotalQuestions</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Total Question Levels -->
            <div class="col-xxl-4 col-md-4">
                <div class="card info-card revenue-card">
                    <div class="card-body">
                        <h5 class="card-title">Total Question Levels</h5>
                        <div class="d-flex align-items-center">
                            <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                                <i class="bi bi-bar-chart"></i>
                            </div>
                            <div class="ps-3">
                                <h5>@Model.TotalQuestionLevels</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Recent Quizzes Table -->
        <div class="row">
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Recent 10 Quizzes</h5>
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Quiz ID</th>
                                    <th>Quiz Name</th>
                                    <th>Total Questions</th>
                                    <th>Quiz Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var quiz in Model.RecentQuizzes)
                                {
                                    <tr>
                                        <td>@quiz.QuizID</td>
                                        <td>@quiz.QuizName</td>
                                        <td>@quiz.TotalQuestions</td>
                                        <td>@quiz.QuizDate</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

            <!-- Recent Questions Table -->
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Recent 10 Questions</h5>
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Question ID</th>
                                    <th>Question</th>
                                    <th>Marks</th>
                                    <th>Correct Option</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var question in Model.RecentQuestions)
                                {
                                    <tr>
                                        <td>@question.QuestionID</td>
                                        <td>@question.QuestionText</td>
                                        <td>@question.QuestionMarks</td>
                                        <td>@question.CorrectOption</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>
 

</main><!-- End #main -->


